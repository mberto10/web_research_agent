modules = ["nodejs-20", "python-3.12"]
[agent]
expertMode = true

[nix]
channel = "stable-25_05"
packages = ["libxcrypt", "libyaml", "postgresql"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "api-server"

[[workflows.workflow]]
name = "verify-setup"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python verify_setup.py"

[workflows.workflow.metadata]
outputType = "console"

[[workflows.workflow]]
name = "api-server"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python run_api.py"

[workflows.workflow.metadata]
outputType = "console"

